<UserControl x:Class="Nova.LED.StadiumBrightnessTool.LayoutTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:mui="http://hardborn.com/UI/MetroUI"
             d:DesignHeight="300"
             d:DesignWidth="300"
             mc:Ignorable="d">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="Tab" SelectedSource="/LocationProfileView.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="配置模式" Source="/LocationProfileView.xaml" />
                <mui:Link DisplayName="调节模式" Source="" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
